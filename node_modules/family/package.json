{
  "name": "family",
  "version": "0.1.1",
  "author": "Family",
  "homepage": "https://docs.family.co",
  "license": "Apache-2.0",
  "description": "Your favorite crypto wallet.",
  "type": "module",
  "main": "./lib/sdk.js",
  "types": "./lib/sdk.d.ts",
  "exports": {
    ".": {
      "import": "./lib/sdk.js",
      "types": "./lib/sdk.d.ts",
      "default": "./lib/sdk.js"
    },
    "./react": {
      "import": "./lib/react.js",
      "types": "./lib/react.d.ts",
      "default": "./lib/react.js"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=12.4"
  },
  "files": [
    "lib",
    "react",
    "package.json"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/post-robot": "10.0.3",
    "@types/react": "^18.0.6",
    "@types/react-dom": "^18.0.2",
    "post-robot": "10.0.46",
    "rollup": "^4.21.3",
    "rollup-plugin-dts-bundle-generator": "^1.4.0",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "@pocket-wallet/custom-types": "1.0.0",
    "@pocket-wallet/prettier-config": "1.0.0",
    "@pocket-wallet/web-elements-bindings": "1.0.30",
    "@pocket-wallet/utils": "1.0.0",
    "@pocket-wallet/sdk-common": "1.0.0",
    "@pocket-wallet/eslint-config": "1.0.0"
  },
  "peerDependencies": {
    "react": "17.x || 18.x || 19.x",
    "react-dom": "17.x || 18.x || 19.x",
    "viem": "2.x",
    "wagmi": "2.x"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "wagmi": {
      "optional": true
    },
    "viem": {
      "optional": true
    }
  },
  "scripts": {
    "clean": "rm -rf lib",
    "build:bundle": "rollup -c rollup.config.mjs",
    "build:watch": "rollup -c rollup.config.mjs --watch",
    "package": "pnpm run build:bundle && pnpm pack",
    "tsc": "tsc --noEmit",
    "eslint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "eslint:fix": "pnpm run eslint --fix",
    "prettier": "prettier --check '**/*.{ts,tsx,js,json,md}'",
    "prettier:fix": "prettier --write '**/*.{ts,tsx,js,json,md}' --loglevel warn",
    "lint": "pnpm run prettier && pnpm run eslint && pnpm run tsc",
    "lint:fix": "pnpm run prettier:fix && pnpm run eslint:fix && pnpm run tsc"
  }
}